{
  "name": "indiana-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "format:test": "prettier --list-different \"./projects/**/*.{ts,html,scss,json}\"",
    "format:write": "prettier --write \"./projects/**/*.{ts,html,scss,json}\"",
    "analyze": "ng build --stats-json --output-hashing=none --named-chunks && esbuild-visualizer --template treemap --metadata dist/backoffice/stats.json --filename dist/backoffice/analyse/index.html  && http-server -o -c-1 ./dist/backoffice/analyse/",
    "analyze:sme": "ng build --source-map --output-hashing=none --named-chunks && source-map-explorer dist/backoffice/browser/*.js --html dist/backoffice/sme/index.html && http-server -o -c-1 ./dist/backoffice/sme/",
    "analyze:deps": "npm-run-all analyze:deps:*",
    "analyze:deps:all": "madge projects/backoffice/src/main.ts --ts-config tsconfig.json --image ./deps/backoffice/_all.png",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.1.1",
    "@angular/cdk": "19.1.0",
    "@angular/common": "19.1.1",
    "@angular/compiler": "19.1.1",
    "@angular/core": "19.1.1",
    "@angular/forms": "19.1.1",
    "@angular/material": "19.1.0",
    "@angular/platform-browser": "19.1.1",
    "@angular/platform-browser-dynamic": "19.1.1",
    "@angular/router": "19.1.1",
    "tslib": "2.8.1",
    "rxjs": "7.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.1.2",
    "@angular/cli": "19.1.2",
    "@angular/compiler-cli": "19.1.1",
    "@types/jasmine": "5.1.5",
    "angular-eslint": "19.0.2",
    "autoprefixer": "10.4.20",
    "esbuild-visualizer": "0.7.0",
    "eslint": "9.18.0",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-boundaries": "5.0.1",
    "http-server": "14.1.1",
    "jasmine-core": "5.5.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "madge": "8.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.5.1",
    "prettier": "3.4.2",
    "source-map-explorer": "2.5.3",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.3",
    "typescript-eslint": "8.20.0"
  }
}
